{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "TokenKey": "some long secret key to sign jwt tokens with",
  "ApiKey": "local-dev-api-key",
  "Auth": {
    "Jwt": {
      "ExpiresMinutes": 60
    },
    "Cookies": {
      "ExpiresMinutes": 60,
      "SlidingExpiration": true,
      "Name": "auth"
    }
  }
}